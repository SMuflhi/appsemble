{
  "private": true,
  "workspaces": [
    "blocks/*",
    "packages/*"
  ],
  "scripts": {
    "appsemble": "ts-node -TP packages/cli/tsconfig.json packages/cli/src",
    "build:app": "webpack --config config/webpack/app.js --mode production",
    "build:studio": "webpack --config config/webpack/studio.js --mode production",
    "create-appsemble": "ts-node -TP packages/create-appsemble/tsconfig.json packages/create-appsemble/src",
    "extract-messages:app": "extract-messages -l da,de,fr,nl,en-US -d en-US -o ./packages/app/translations --flat './packages/app/src/**/messages.ts'",
    "extract-messages:react-components": "extract-messages -l da,de,fr,nl,en-US -d en-US -o ./packages/react-components/translations --flat './packages/react-components/src/**/messages.ts'",
    "extract-messages:studio": "extract-messages -l da,de,fr,nl,en-US -d en-US -o ./packages/studio/translations --flat './packages/studio/src/**/messages.ts'",
    "prettier": "prettier --check",
    "scripts": "ts-node -TP packages/scripts/tsconfig.json packages/scripts/src",
    "start": "ts-node -TP packages/cli/tsconfig.json packages/cli/src start",
    "stylelint": "stylelint --report-needless-disables",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --report-needless-disables"
    ],
    "*.md": [
      "remark --frail --no-stdout"
    ],
    "*.{html,json,md,scss,yaml,yml}": [
      "prettier --check"
    ],
    "*.{js,md,ts,tsx}": [
      "eslint"
    ]
  },
