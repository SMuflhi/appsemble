{
  "name": "@appsemble/map",
  "version": "1.0.0",
  "private": true,
  "appsemble": {
    "actions": {
      "markerClick": {}
    },
    "output": "dist",
    "resources": {
      "marker": {
        "schema": {
          "type": "object",
          "required": [
            "lat",
            "lon"
          ],
          "properties": {
            "lat": {
              "type": "number",
              "minimum": "-90",
              "maximum": "90"
            },
            "lon": {
              "type": "number",
              "minimum": "-180",
              "maximum": "180"
            },
            "data": {}
          }
        }
      }
    }
  },
  "dependencies": {
    "@appsemble/sdk": "^1.0.0",
    "@appsemble/utils": "^1.0.0",
    "leaflet": "^1.4.0"
  }
}
