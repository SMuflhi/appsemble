{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "noEmit": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictFunctionTypes": false,
    "strictNullChecks": false,
    "lib": ["esnext"],
    "types": ["jest", "node"]
  },
  "exclude": ["blocks/*", "packages/*"]
}
