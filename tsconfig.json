{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "noEmit": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictFunctionTypes": false,
    "strictNullChecks": false
  },
  "exclude": ["blocks/*", "packages/*"]
}
